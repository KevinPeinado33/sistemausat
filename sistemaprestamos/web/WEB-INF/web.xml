<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PrestamoController</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.PrestamoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pr</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.prueba</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfesorController</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.ProfesorController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pro</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.ProductoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DetallePrestamoController</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.DetallePrestamoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReservasController</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.ReservasController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.Main</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.Home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Close</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.Close</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuCon</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.UsuCon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioController</servlet-name>
        <servlet-class>pe.edu.upeu.presup.controller.UsuarioController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PrestamoController</servlet-name>
        <url-pattern>/Pc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pr</servlet-name>
        <url-pattern>/pr</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfesorController</servlet-name>
        <url-pattern>/pc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetallePrestamoController</servlet-name>
        <url-pattern>/DPC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReservasController</servlet-name>
        <url-pattern>/rc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/menu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Close</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuCon</servlet-name>
        <url-pattern>/UsuCon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioController</servlet-name>
        <url-pattern>/uc</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
